package com.aggregator.news.constants;

public enum NewsCategory {

	TECHNOLOGY("technology:tech:IT industry"), HEALTHCARE("health:hospital:medication:meditation"), ENTERTAINMENT(
			"entertainment:films:music:movies"), BUSINESS("business:stocks:share market");

	private String category;

	NewsCategory(String category) {
		this.category = category;
	}

	public String getCategory() {
		return category;
	}

}
